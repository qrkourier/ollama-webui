ollama.bingnet.cloud, ollama.ziti.bingnet.cloud {
        tls {
                dns cloudflare {env.CF_API_TOKEN}
                resolvers 1.1.1.1
        }

        log {
                output stdout
                format console
                level INFO
                #level DEBUG
        }

        reverse_proxy /* http://ollama:11434
}

